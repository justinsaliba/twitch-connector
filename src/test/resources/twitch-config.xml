<?xml version="1.0" encoding="UTF-8"?>

<mule xmlns="http://www.mulesoft.org/schema/mule/core"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xmlns:spring="http://www.springframework.org/schema/beans"
      xmlns:twitch="http://www.mulesoft.org/schema/mule/twitch"
      xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
        http://www.mulesoft.org/schema/mule/core http://www.mulesoft.org/schema/mule/core/current/mule.xsd
        http://www.mulesoft.org/schema/mule/twitch http://www.mulesoft.org/schema/mule/twitch/1.0/mule-twitch.xsd">

    <twitch:config-type name="config" greeting="Hi"/>

    <flow name="testFlow">
        <twitch:greet config-ref="config" friend="Foo"/>
    </flow>
</mule>
